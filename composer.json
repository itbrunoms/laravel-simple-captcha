{
  "name": "itbrunoms/laravel-simple-captcha",
  "description": "A laravel simple captcha package",
  "license": "cc-by-4.0",
  "keywords": [
    "laravel",
    "simple",
    "captcha"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Bruno Moraes",
      "email": "contato@programadorbruno.com.br"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Itbrunoms\\LaravelSimpleCaptcha\\": "src"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Itbrunoms\\LaravelSimpleCaptcha\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
